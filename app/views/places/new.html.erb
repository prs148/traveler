

<div class= "booyah-box col-12">
  <br />
  <h1> Add Your Place </h1>
  <div class= "place_form" >
    <%= simple_form_for @place do |f| %>
      <%= f.input :title %>
      <%= f.input :description %>
      <%= f.input :address %>
      <%= f.submit 'Create', class:  'btn primary btn' %>

    <% end %> 
  </div>

</div> 